Global tiledata:Int[256]

Function init_tiledata()

	tiledata[0] = 1
	tiledata[3] = 1
	tiledata[4] = 1	
	tiledata[5] = 1	
	tiledata[8] = 1
	tiledata[9] = 1	
	tiledata[10] = 1
	For Local t:Int = 14 To 27			
		tiledata[t] = 1
	Next
	tiledata[34] = 1
	tiledata[35] = 1
	tiledata[36] = 1
	tiledata[37] = 1
	tiledata[38] = 1	
	tiledata[39] = 1	
						
EndFunction
